@model IEnumerable<Homework7.Models.RequestLog>

@{
    ViewBag.Title = "ListOfQueries";
}

<h2>ListOfQueries</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TimeNow)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Request)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BrowserType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TimeNow)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Request)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BrowserType)
        </td>

    </tr>
}

</table>
